public class Ventana_atributos_dibujo extends JInternalFrame
{
  JComboBox cTipoTrazo = new JComboBox();
  JLabel lGrosor = new JLabel();
  JSlider sTamaFuente = new JSlider();
  JComboBox cRelleno = new JComboBox();
  JSlider sGrosor = new JSlider();
  BorderLayout borderLayout8 = new BorderLayout();
  JLabel lRelleno = new JLabel();
  GridLayout gridLayout2 = new GridLayout(4,1);
  JPanel jPanelContenedor = new JPanel();
  JLabel lTipoTrazo = new JLabel();
  FlowLayout flowLayout1 = new FlowLayout();
  JPanel jPanelAtributos = new JPanel();
  JLabel lTamaFuente = new JLabel();
  JPanel jPanelFormas = new JPanel();
  JPanel jPanelFormasAtributos = new JPanel();
  JLabel lFuente = new JLabel();
  JComboBox cFuentes = new JComboBox();
  BorderLayout borderLayout9 = new BorderLayout();
  GridLayout gridLayout1 = new GridLayout();
  JPanel jPanelColor = new JPanel();
  JLabel jLabel1 = new JLabel();
  BorderLayout borderLayout1 = new BorderLayout();
  JPanel jPanelElegirColores = new JPanel();
  GridLayout gridLayout3 = new GridLayout(2,2);
  JPanel jPanelNegroRelleno = new JPanel();
  JPanel jPanelNaranjaRelleno = new JPanel();
  JPanel jPanelRosaRelleno = new JPanel();
  JPanel jPanelAzulRelleno = new JPanel();
  JPanel jPanelVerdeRelleno = new JPanel();
  JPanel jPanelAmarilloRelleno = new JPanel();
  JPanel jPanelRojoRelleno = new JPanel();
  JPanel jPanelBlancoRelleno = new JPanel();
  JPanel jPanelMarronRelleno = new JPanel();
  JPanel jPanelVioletaRelleno = new JPanel();
  JPanel jPanelAzulClaroRelleno = new JPanel();
  JPanel jPanelLimaRelleno = new JPanel();
  JPanel jPanelBeigeRelleno = new JPanel();
  JPanel jPanelGrisClaroRelleno = new JPanel();
  JPanel jPanelColorTrazo = new JPanel();
  BorderLayout borderLayout2 = new BorderLayout();
  JLabel jLabel2 = new JLabel();
  JPanel jPanelElegirColoresTrazo = new JPanel();
  GridLayout gridLayout4 = new GridLayout(2,2);
  JPanel jPanelNegroTrazo = new JPanel();
  JPanel jPanelNaranjaTrazo = new JPanel();
  JPanel jPanelRosaTrazo = new JPanel();
  JPanel jPanelAzulTrazo = new JPanel();
  JPanel jPanelVerdeTrazo = new JPanel();
  JPanel jPanelAmarilloTrazo = new JPanel();
  JPanel jPanelRojoTrazo = new JPanel();
  JPanel jPanelBlancoTrazo = new JPanel();
  JPanel jPanelMarronTrazo = new JPanel();
  JPanel jPanelVioletaTrazo = new JPanel();
  JPanel jPanelAzulClaroTrazo = new JPanel();
  JPanel jPanelLimaTrazo = new JPanel();
  JPanel jPanelBeigeTrazo = new JPanel();
  JPanel jPanelGrisClaroTrazo = new JPanel();
  JPanel jPanelColorRelleno = new JPanel();
  BorderLayout borderLayout3 = new BorderLayout();
  JPanel jPanelColoresActuales = new JPanel();
  JPanel jPanel1 = new JPanel();
  BorderLayout borderLayout4 = new BorderLayout();
  JLabel jLabel3 = new JLabel();
  JLabel jLabel4 = new JLabel();
  FlowLayout flowLayout2 = new FlowLayout();
  JPanel jPanelMostrarColorActualRelleno = new JPanel();
  JLabel jLabel5 = new JLabel();
  JPanel jPanelMostrarColorActualTrazo = new JPanel();
  Interfaz creador = null;

  // ***** Variables privadas de la clase *****
  private Color trazo_personalizado, relleno_personalizado, negro, rojo, azul, blanco, amarillo, gris, verde, rosa, marron, naranja, azul_claro, beige, lima, violeta, color_actual_relleno, color_actual_trazo;
  private ButtonGroup grupo = new ButtonGroup ();
  // ******************************************

//Modificado para poder salir cuando se cierra la ventana
  protected void processWindowEvent(WindowEvent e)
  {
    //super.processWindowEvent(e);
    if (e.getID() == WindowEvent.WINDOW_CLOSING)
    {
      creador.mostrar_infor("cerrada la ventana de herramientas");
      /*int i = JOptionPane.showConfirmDialog(this, "¿Cerrar Super-Paint?", "Salir", JOptionPane.YES_NO_OPTION);
      if( i == 0 )
      {
        System.exit(0);
      }
      else
        this.setVisible(true);*/
    }
  }

  public Ventana_atributos_dibujo(Interfaz inter)
  {
    creador = inter;
    try
    {
      jbInit();
    }
    catch(Exception ex)
    {
      ex.printStackTrace();
    }
  }

  void sGrosor_mouseDragged(MouseEvent e)
  {
    lGrosor.setText("Grosor de línea: " + sGrosor.getValue());
    creador.setGrosor(sGrosor.getValue());
  }

  void sTamaFuente_mouseDragged(MouseEvent e)
  {
    lTamaFuente.setText("Tamaño fuente: " + sTamaFuente.getValue());
    creador.setTamaFuente(sTamaFuente.getValue());
  }

  public Ventana_atributos_dibujo() {
    try
    {
      jbInit();
    }
    catch(Exception e) {
      e.printStackTrace();
    }
  }

  private void jbInit() throws Exception
  {
    this.setTitle("Herramientas");
    this.setClosable(true);
    this.setIconifiable(true);
    // ***** Pongo los iconos *****
    ImageIcon circulo = new ImageIcon("iconos/circulo.gif");
    ImageIcon linea = new ImageIcon("iconos/linea.gif");
    ImageIcon rectangulo = new ImageIcon("iconos/rectangulo.gif");
    ImageIcon arco = new ImageIcon("iconos/arco.gif");
    ImageIcon trazolibre = new ImageIcon("iconos/trazo_libre.gif");
    ImageIcon texto = new ImageIcon("iconos/texto.gif");
    ImageIcon rectanguloredondeado = new ImageIcon("iconos/rectangulo_redondeado.gif");
    ImageIcon punto = new ImageIcon("iconos/punto.gif");
    ImageIcon estrella = new ImageIcon("iconos/estrella.gif");
    ImageIcon cpc = new ImageIcon("iconos/cpc.gif");
    ImageIcon goma = new ImageIcon("iconos/goma.gif");
    tbBotonElipse.setIcon(circulo);
    tbBotonLinea.setIcon(linea);
    tbBotonRectangulo.setIcon(rectangulo);
    tbBotonArco.setIcon(arco);
    tbBotonTrazoLibre.setIcon(trazolibre);
    tbBotonTexto.setIcon(texto);
    tbBotonRectanguloRedondeado.setIcon(rectanguloredondeado);
    tbBotonPunto.setIcon(punto);
    tbBotonEstrella.setIcon(estrella);
    tbBotonCPC.setIcon(cpc);
    tbBotonGoma.setToolTipText("Goma de borrar");
    tbBotonGoma.setIcon(goma);
    tbBotonGoma.addMouseListener(new Ventana_atributos_dibujo_tbBotonGoma_mouseAdapter(this));

    ImageIcon negrita = new ImageIcon("iconos/negrita.gif");
    ImageIcon cursiva = new ImageIcon("iconos/cursiva.gif");
    ImageIcon subrayado = new ImageIcon("iconos/subrayado.gif");

    // ****************************

    // ***** Cargo la lista de fuentes instaladas en el equipo *****
    GraphicsEnvironment env = GraphicsEnvironment.getLocalGraphicsEnvironment();
    String[] fuentes = env.getAvailableFontFamilyNames();
    for(int i = 0; i < fuentes.length; i++)
      cFuentes.addItem(fuentes[i]);

    creador.setFuente( (String) cFuentes.getSelectedItem() ); // pongo la fuente inicial
    // *************************************************************

    // ***** Agrupo los botones *****
    lNegrita.setIcon(negrita);
    cbCursiva.addMouseListener(new Ventana_atributos_dibujo_cbCursiva_mouseAdapter(this));
    cbCursiva.addMouseListener(new Ventana_atributos_dibujo_cbCursiva_mouseAdapter(this));
    lCursiva.setIcon(cursiva);
    jPanelAtributosTexto.setLayout(borderLayout10);
    cbNegrita.addMouseListener(new Ventana_atributos_dibujo_cbNegrita_mouseAdapter(this));
    cbNegrita.addMouseListener(new Ventana_atributos_dibujo_cbNegrita_mouseAdapter(this));
    lSubrayado.setIcon(subrayado);
    cbSubrayado.addMouseListener(new Ventana_atributos_dibujo_cbSubrayado_mouseAdapter(this));
    cbSubrayado.addMouseListener(new Ventana_atributos_dibujo_cbSubrayado_mouseAdapter(this));
    cbNegrita.setToolTipText("Negrita");
    cbCursiva.setToolTipText("Cursiva");
    cbSubrayado.setToolTipText("Subrayado");
    jPanelElegirTrazoPersonalizado.setLayout(borderLayout5);
    jLabel7.setText("Elegir Trazo");
    jPanelTrazoPersonalizado.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelTrazoPersonalizado.setLayout(flowLayout4);
    jPanelTrazoPersonalizado.addMouseListener(new Ventana_atributos_dibujo_jPanelTrazoPersonalizado_mouseAdapter(this));
    jLabel8.setText("Elegir relleno:");
    jPanelRellenoPersonalizado.setLayout(borderLayout6);
    jPanelElegirRellenoPersonalizado.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelElegirRellenoPersonalizado.addMouseListener(new Ventana_atributos_dibujo_jPanelElegirRellenoPersonalizado_mouseAdapter(this));
    grupo.add(tbBotonPunto);
    grupo.add(tbBotonLinea);
    grupo.add(tbBotonRectangulo);
    grupo.add(tbBotonElipse);
    grupo.add(tbBotonRectanguloRedondeado);
    grupo.add(tbBotonArco);
    grupo.add(tbBotonCPC);
    grupo.add(tbBotonTexto);
    grupo.add(tbBotonEstrella);
    grupo.add(tbBotonTrazoLibre);
    grupo.add(tbBotonGoma);
    // ******************************

    // ***** Le doy su valor a los colores *****
    negro = Color.black;
    rojo = Color.red;
    azul = Color.blue;
    blanco = Color.white;
    amarillo = Color.yellow;
    gris = Color.gray;
    verde = Color.green;
    rosa = Color.pink;
    marron = new Color(165,42,42);
    naranja = Color.orange;
    azul_claro = new Color(176,224,230);
    beige = new Color(245,245,220);
    lima = new Color(32,205,32);
    violeta = new Color(221,160,221);
    color_actual_trazo = negro;
    color_actual_relleno = blanco;
    // ******************************************

    // ***** Pongo los colores en sus respectivos paneles *****
    jPanelNegroRelleno.setBackground(negro);
    jPanelNegroTrazo.setBackground(negro);
    jPanelGrisClaroRelleno.setBackground(gris);
    jPanelGrisClaroTrazo.setBackground(gris);
    jPanelBlancoRelleno.setBackground(blanco);
    jPanelBlancoTrazo.setBackground(blanco);
    jPanelRojoRelleno.setBackground(rojo);
    jPanelRojoTrazo.setBackground(rojo);
    jPanelAmarilloRelleno.setBackground(amarillo);
    jPanelAmarilloTrazo.setBackground(amarillo);
    jPanelVerdeRelleno.setBackground(verde);
    jPanelVerdeTrazo.setBackground(verde);
    jPanelAzulRelleno.setBackground(azul);
    jPanelAzulTrazo.setBackground(azul);
    jPanelRosaRelleno.setBackground(rosa);
    jPanelRosaTrazo.setBackground(rosa);
    jPanelNaranjaRelleno.setBackground(naranja);
    jPanelNaranjaTrazo.setBackground(naranja);
    jPanelMarronRelleno.setBackground(marron);
    jPanelMarronTrazo.setBackground(marron);
    jPanelAzulClaroRelleno.setBackground(azul_claro);
    jPanelAzulClaroTrazo.setBackground(azul_claro);
    jPanelBeigeRelleno.setBackground(beige);
    jPanelBeigeTrazo.setBackground(beige);
    jPanelLimaRelleno.setBackground(lima);
    jPanelLimaTrazo.setBackground(lima);
    jPanelVioletaRelleno.setBackground(violeta);
    jPanelVioletaTrazo.setBackground(violeta);
    jPanelMostrarColorActualRelleno.setBackground(color_actual_relleno);
    jPanelMostrarColorActualRelleno.setToolTipText("Color actual de relleno");
    jPanelMostrarColorActualTrazo.setBackground(color_actual_trazo);
    jPanelMostrarColorActualTrazo.setToolTipText("Color actual de trazo");
    // ***********************************************************


    this.setResizable(false); // deshabilito la opción de maximizar
    setDefaultCloseOperation(0); // deshabilito la opción de cerrar
    jPanelContenedor.setLayout(borderLayout9);
    lRelleno.setText("Tipo de relleno:");
    sGrosor.setToolTipText("");
    sGrosor.setPreferredSize(new Dimension(90, 24));
    sGrosor.setMaximumSize(new Dimension(90, 24));
    sGrosor.setBorder(BorderFactory.createRaisedBevelBorder());
    sGrosor.setMajorTickSpacing(1);
    sGrosor.setMaximum(10);
    sGrosor.setMinimum(1);
    sGrosor.setValue(3);
    sGrosor.addMouseMotionListener(new Ventana_atributos_dibujo_sGrosor_mouseMotionAdapter(this));
    sTamaFuente.setValue(12);
    sTamaFuente.addMouseMotionListener(new Ventana_atributos_dibujo_sTamaFuente_mouseMotionAdapter(this));
    sTamaFuente.setPreferredSize(new Dimension(90, 24));
    sTamaFuente.setMaximumSize(new Dimension(90, 24));
    sTamaFuente.setMaximum(70);
    lGrosor.setText("Grosor de línea: 3");
    this.getContentPane().setLayout(gridLayout1);
    cTipoTrazo.addItem("Continuo");
    cTipoTrazo.addItem("Punteado");
    cTipoTrazo.addItem("Redondeado");
    cRelleno.addItem("Sin relleno");
    cRelleno.addItem("Liso");
    cRelleno.addItem("Degradado");
    lTipoTrazo.setText("Tipo de trazo:");
    jPanelAtributos.setLayout(flowLayout1);
    jPanelAtributos.setBorder(BorderFactory.createLoweredBevelBorder());
    lTamaFuente.setText("Tamaño fuente: 12");
    jPanelFormas.setLayout(gridLayout2);
    jPanelFormas.setBorder(BorderFactory.createLoweredBevelBorder());
    jPanelFormasAtributos.setLayout(borderLayout8);
    jPanelFormasAtributos.setBorder(BorderFactory.createEtchedBorder());
    jPanelFormasAtributos.setMaximumSize(new Dimension(100, 100));
    jPanelFormasAtributos.setMinimumSize(new Dimension(142, 153));
    jPanelFormasAtributos.setPreferredSize(new Dimension(100, 100));
    lFuente.setText("Tipo de fuente:");
    jPanelColor.setBorder(BorderFactory.createLoweredBevelBorder());
    jPanelColor.setLayout(borderLayout1);
    cTipoTrazo.setPreferredSize(new Dimension(100, 19));
    cRelleno.setPreferredSize(new Dimension(100, 19));
    cFuentes.setPreferredSize(new Dimension(100, 19));
    jLabel1.setText("Color del relleno:");
    jPanelElegirColores.setLayout(gridLayout3);
    jPanelNegroRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelNegroRelleno.setToolTipText("Negro");
    jPanelNegroRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelNegroRelleno_mouseAdapter(this));
    jPanelBlancoRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelBlancoRelleno.setToolTipText("Blanco");
    jPanelBlancoRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelBlancoRelleno_mouseAdapter(this));
    jPanelRojoRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelRojoRelleno.setToolTipText("Rojo");
    jPanelRojoRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelRojoRelleno_mouseAdapter(this));
    jPanelAmarilloRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelAmarilloRelleno.setToolTipText("Amarillo");
    jPanelAmarilloRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelAmarilloRelleno_mouseAdapter(this));
    jPanelVerdeRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelVerdeRelleno.setToolTipText("Verde");
    jPanelVerdeRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelVerdeRelleno_mouseAdapter(this));
    jPanelAzulRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelAzulRelleno.setDebugGraphicsOptions(0);
    jPanelAzulRelleno.setToolTipText("Azul");
    jPanelAzulRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelAzulRelleno_mouseAdapter(this));
    jPanelRosaRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelRosaRelleno.setToolTipText("Rosa");
    jPanelRosaRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelRosaRelleno_mouseAdapter(this));
    jPanelNaranjaRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelNaranjaRelleno.setRequestFocusEnabled(true);
    jPanelNaranjaRelleno.setToolTipText("Naranja");
    jPanelNaranjaRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelNaranjaRelleno_mouseAdapter(this));
    jPanelVioletaRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelVioletaRelleno.setToolTipText("Violeta");
    jPanelVioletaRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelVioletaRelleno_mouseAdapter(this));
    jPanelMarronRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelMarronRelleno.setToolTipText("Marrón");
    jPanelMarronRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelMarronRelleno_mouseAdapter(this));
    jPanelGrisClaroRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelGrisClaroRelleno.setToolTipText("Gris");
    jPanelGrisClaroRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelGrisClaroRelleno_mouseAdapter(this));
    jPanelBeigeRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelBeigeRelleno.setToolTipText("Beige");
    jPanelBeigeRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelBeigeRelleno_mouseAdapter(this));
    jPanelLimaRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelLimaRelleno.setToolTipText("Lima");
    jPanelLimaRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelLimaRelleno_mouseAdapter(this));
    jPanelAzulClaroRelleno.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelAzulClaroRelleno.setToolTipText("Azul claro");
    jPanelAzulClaroRelleno.addMouseListener(new Ventana_atributos_dibujo_jPanelAzulClaroRelleno_mouseAdapter(this));
    jPanelColorTrazo.setLayout(borderLayout2);
    jLabel2.setText("Color del trazo:");
    jPanelElegirColoresTrazo.setLayout(gridLayout4);
    jPanelNegroTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelNegroTrazo.setToolTipText("Negro");
    jPanelNegroTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelNegroTrazo_mouseAdapter(this));
    jPanelRosaTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelRosaTrazo.setDebugGraphicsOptions(0);
    jPanelRosaTrazo.setToolTipText("Rosa");
    jPanelRosaTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelRosaTrazo_mouseAdapter(this));
    jPanelNaranjaTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelNaranjaTrazo.setToolTipText("Naranja");
    jPanelNaranjaTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelNaranjaTrazo_mouseAdapter(this));
    jPanelAzulTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelAzulTrazo.setToolTipText("Azul");
    jPanelAzulTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelAzulTrazo_mouseAdapter(this));
    jPanelVerdeTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelVerdeTrazo.setToolTipText("Verde");
    jPanelVerdeTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelVerdeTrazo_mouseAdapter(this));
    jPanelAmarilloTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelAmarilloTrazo.setToolTipText("Amarillo");
    jPanelAmarilloTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelAmarilloTrazo_mouseAdapter(this));
    jPanelRojoTrazo.setBorder(BorderFactory.createRaisedBevelBorder());
    jPanelRojoTrazo.setToolTipText("Rojo");
    jPanelRojoTrazo.addMouseListener(new Ventana_atributos_dibujo_jPanelRojoTrazo_mouseAdapter(this));
