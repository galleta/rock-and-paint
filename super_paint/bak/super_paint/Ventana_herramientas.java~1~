public class Ventana_herramientas extends JFrame {
  BorderLayout borderLayout1 = new BorderLayout();

  public Ventana_herramientas() {
    try {
      jbInit();
    }
    catch(Exception ex) {
      ex.printStackTrace();
    }
  }

  void jbInit() throws Exception {
    this.getContentPane().setLayout(borderLayout1);
  }
}
