public class Ventana_dibujo extends JFrame
{
  BorderLayout borderLayout1 = new BorderLayout();
  JCheckBox jCheckBox1 = new JCheckBox();

  public Ventana_dibujo(ArrayList colores, Interfaz creador)
  {
    try
    {
      jbInit(colores);
    }
    catch(Exception ex)
    {
      ex.printStackTrace();
    }
  }

  void jbInit(ArrayList colores) throws Exception
  {
    jCheckBox1.setText("jCheckBox1");
    this.getContentPane().add(jCheckBox1, BorderLayout.NORTH);
  }


  private void jbInit() throws Exception {
    jCheckBox1.setText("jCheckBox1");
    this.getContentPane().add(jCheckBox1, BorderLayout.NORTH);
  }
}
