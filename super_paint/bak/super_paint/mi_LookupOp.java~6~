public class mi_LookupOp extends objeto
{
  private LookupOp oper;
  Raster ra;

  public mi_LookupOp() {
  }

  public mi_LookupOp(Raster r)
  {
    ra = r;
  }

  public mi_LookupOp(LookupOp operacion)
  {
    oper = operacion;
  }

  public LookupOp getFiltro()
  {
    return oper;
  }

  public Raster getIma()
  {
    return ra;
  }

  /**
   * getColor
   *
   * @return Color
   */
  public Color getColor() {
    return null;
  }

  /**
   * getTama
   *
   * @return int
   */
  public int getTama() {
    return 0;
  }

}
